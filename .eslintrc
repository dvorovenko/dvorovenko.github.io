{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "generators": true,
      "modules": true,
      "objectLiteralDuplicateProperties": true
    }
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true
    }]
  }
}
